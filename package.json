{
  "name": "startup-village-county",
  "version": "1.0.0",
  "description": "Startup Village County - Property, Events, and Products Platform",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server": "cd backend && npm start",
    "server:dev": "cd backend && npm run dev",
    "server:prod": "cd backend && node production-server.js",
    "install:all": "npm install && cd backend && npm install",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "start:prod": "npm run build && npm run server:prod",
    "deploy": "npm run install:all && npm run build && pm2 start ecosystem.config.js --env production",
    "build:prod": "node build-production.js",
    "restart": "pm2 restart startup-village-backend",
    "stop": "pm2 stop startup-village-backend",
    "logs": "pm2 logs startup-village-backend",
    "status": "pm2 status"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "swiper": "^11.2.10"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^7.6.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "vite": "^4.1.0"
  }
}
